<!DOCTYPE html>
<html lang="en">

  <head>
    <% include ./partials/head %>
  </head>

  <body id="page-top">
    <nav class="navbar navbar-expand-lg navbar-dark bg-primary fixed-top" id="sideNav">
      <% include ./partials/navbar %>
    </nav>

    <div class="container-fluid p-0" style="width:69%;">
      <section class="resume-section p-3 p-lg-5 d-flex d-column" id="about">
        <div class="my-auto">
          <% var name = data.firstname +  " " + data.middlename + " " + data.lastname; %>
          <h2 class="mb-0" id="teacher-name"><%= name %></h2>
          <div class="subheading mb-4 " id="title"><%= data.title %></div>
          <div class="subheading " id="dob"><%= data.dob %></div>
          <div class="subheading " id="address"><%= data.address %></div>
          <div class="subheading" id="email"><%= data.email %></div>
          <div class="subheading" id="phone"><%= data.address %></div>
        </div>
      </section>
    </div>   

    <% include ./partials/bootjs %>


  </body>

</html>
